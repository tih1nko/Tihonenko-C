<Window x:Class="QA_Auto__Trainee_.ManualSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QA_Auto__Trainee_"
        mc:Ignorable="d"
        Title="Заполнение массива" Height="190" Width="475">
    <Grid>
        <TextBox x:Name="TB_AddNum" HorizontalAlignment="Left" Height="23" Margin="10,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="447"/>
        <Button x:Name="B_AddToArray" Content="Добавить значение" HorizontalAlignment="Left" Margin="196,62,0,0" VerticalAlignment="Top" Width="128" Click="B_AddToArray_Click"/>
        <Button x:Name="B_PrevDell" Content="Удалить предыдущее" HorizontalAlignment="Left" Margin="196,87,0,0" VerticalAlignment="Top" Width="128" Click="B_PrevDell_Click"/>
        <Button x:Name="B_ClearArray" Content="Очистить" HorizontalAlignment="Left" Margin="329,62,0,0" VerticalAlignment="Top" Width="128" Click="B_ClearArray_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,18,0,0" TextWrapping="Wrap" Text="Введите число" VerticalAlignment="Top"/>
        <TextBlock x:Name="TextB_Array" HorizontalAlignment="Left" Margin="10,113,0,0" TextWrapping="Wrap" Text="Массив: " VerticalAlignment="Top"/>
        </Grid>
</Window>
